@page "/"
@{
    Layout = null;
}

<!DOCTYPE html>
<html>

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no" />
    <title>CuriousCrowd</title>
    <base href="/" />
    <link href="css/defaults.css" rel="stylesheet" />
    <link href="css/app.css" rel="stylesheet" />
    <link href="css/LoadingScreen.css" rel="stylesheet" />
    <link href="CuriousCrowd.styles.css" rel="stylesheet" />
</head>

<body>
